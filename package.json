{
  "name": "webhook-to-vensi-eventbrige",
  "version": "1.0.0",
  "bin": {
    "app": "bin/app-synth.js"
  },
  "scripts": {
    "watch": "tsc -w",
    "test": "echo 'no tests'",
    "cdk": "cdk",
    "build:app": "tsc",
    "_build:layer": "rm -rf dist/dependencies/ && mkdir -p dist/dependencies/nodejs && cp package.json .npmrc dist/dependencies/nodejs && npm install --production --prefix dist/dependencies/nodejs && rm dist/dependencies/nodejs/package.json && rm dist/dependencies/nodejs/package-lock.json && rm dist/dependencies/nodejs/.npmrc",
    "build": "npm run build:app",
    "cdk:synth": "npx cdk synth",
    "cdk:deploy": "npm run build && npx cdk deploy"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "1.94.0",
    "@aws-cdk/aws-codebuild": "1.94.0",
    "@aws-cdk/aws-codecommit": "1.94.0",
    "@aws-cdk/aws-codepipeline": "1.94.0",
    "@aws-cdk/aws-codepipeline-actions": "1.94.0",
    "@aws-cdk/aws-events": "1.94.0",
    "@aws-cdk/aws-lambda": "1.94.0",
    "@aws-cdk/core": "1.94.0",
    "@aws-cdk/pipelines": "1.94.0",
    "@types/aws-lambda": "^8.10.72",
    "@types/highland": "^2.12.12",
    "@types/lodash.memoize": "^4.1.6",
    "@types/node": "^14",
    "@types/node-fetch": "^2.5.8",
    "aws-cdk": "1.94.0",
    "aws-sdk": "^2.868.0",
    "typescript": "~4.2"
  },
  "dependencies": {
    "source-map-support": "^0.5.19"
  }
}
